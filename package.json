{
  "name": "nodejs-starter",
  "version": "1.1.0",
  "description": "A collection of examples and libraries for speedy Node.js development",
  "main": "index.js",
  "scripts": {
    "debug": "DEBUG=* node . | ./node_modules/bunyan/bin/bunyan -c 'false'",
    "app": "node . | ./node_modules/bunyan/bin/bunyan -o short",
    "test": "NODE_ENV=test mocha --colors",
    "test:watch": "watch-run -i -p 'src/**/*,test/**/*' npm run test",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "coverage": "istanbul cover _mocha"
  },
  "author": "Bj√∂rn Tegelund <bjorn@tumblbug.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.1.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "nock": "^7.5.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "watch-run": "^1.2.4"
  },
  "dependencies": {
    "bunyan": "^1.8.0",
    "debug": "^2.2.0",
    "lodash": "^4.6.1"
  }
}
